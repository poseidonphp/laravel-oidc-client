{
  "name": "poseidonphp/laravel-oidc-client",
  "description": "Provides OIDC authentication to Laravel applications. Works with identity providers such as Ping Federate.",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Poseidonphp\\LaravelOidcClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Poseidonphp\\LaravelOidcClient\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.3",
    "laravel/framework": ">= 8.0",
    "phpseclib/phpseclib": "^3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Dan H",
      "email": "poseidonphp@outlook.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Poseidonphp\\LaravelOidcClient\\LaravelOidcClientServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
